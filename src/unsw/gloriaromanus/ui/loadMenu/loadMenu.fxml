<Pane fx:controller="unsw.gloriaromanus.ui.loadMenu.LoadMenuController" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.4" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="load_game_text" StackPane.alignment="TOP_CENTER" text="Load Game Options">
