      </ComboBox>
      <HBox fx:id="buttons" StackPane.alignment="CENTER" layoutX="290.0" layoutY="300.0" >
        <Button fx:id="cancel_btn" onMouseClicked="#onCancelButton" maxHeight="30.0" prefWidth="100.0" mnemonicParsing="false" text="Cancel" />
        <Button fx:id="load_btn" onMouseClicked="#onLoadButton" maxHeight="30.0" prefWidth="100.0" mnemonicParsing="false" text="Load Game" />
      </HBox>
   </children>
</Pane>
