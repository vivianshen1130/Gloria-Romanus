<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:controller="unsw.gloriaromanus.ui.settingsMenu.SettingsMenuController" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.4" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="text" StackPane.alignment="TOP_CENTER" layoutX="240.0" layoutY="64.0" text="Game Options">
         <font>
            <Font size="32.0" />
         </font> 
      </Label>
      <HBox fx:id="buttons" layoutX="240.0" layoutY="300.0" >
        <Button fx:id="end_btn" onMouseClicked="#onEndButton" maxHeight="30.0" prefWidth="100.0" mnemonicParsing="false" text="End Game" />
        <Button fx:id="play_btn" onMouseClicked="#onPlayButton" maxHeight="30.0" prefWidth="100.0" mnemonicParsing="false" text="Keep Playing" />
        <Button fx:id="save_btn" onMouseClicked="#onSaveButton" maxHeight="30.0" prefWidth="100.0" mnemonicParsing="false" text="Save Game" />
      </HBox>
   </children>
</StackPane>
